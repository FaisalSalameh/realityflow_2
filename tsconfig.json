{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "noEmitOnError": true,
        "sourceMap": true,
        "outDir": "../node/editor/server/bld",
        "lib": [ "es2015" ]
    },
    "exclude": [
        "**/node_modules",
        "**web-client"
    ],
    "include": [
        "**/*.spec.ts"
    ]
}